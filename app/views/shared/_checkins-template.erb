<script id="checkins_template" type="text/html">
  <h3 class="check-ins"> Add Check-ins</h3>
  <div class="span5">
    <table class="table table-condensed">
      <thead>
        <tr>
          <th colspan="3">Select Your Check-ins</th>
        </tr>
      </thead>
      <tbody>
        {{#data}}
        <tr>
          <td><input class="checkin_checkbox" type="checkbox" data-id="{{ id }}"></td>
          <td><img src="{{ venue.categories[0].icon }}" /></td>
          <td>{{ venue.name }}</td>
        </tr>
        <tbody>
        {{/data}}
    </table>
  </div>
</script>